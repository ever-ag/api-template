{
   "compilerOptions": {
      "lib": [
         "es2018"
      ],
      "target": "es2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./built",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false
   },
   "include": ["src"]
}